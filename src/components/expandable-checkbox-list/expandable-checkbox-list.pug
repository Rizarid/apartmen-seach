include __item/expandable-checkbox-list__item.pug
mixin expandableCheckboxList(options)
  -  const title = options.title || "expandable checkbox list"
  article.expandable-checkbox-list
    .expandable-checkbox-list__conteiner-of-title.js-expandable-checkbox-list__conteiner-of-title 
      h3.expandable-checkbox-list__title #{title}
    .expandable-checkbox-list__list  
      each item in options.itemOptions
        +expandableCheckboxList__item(item.checkboxName, item.checkboxChecked, item.itemText)

