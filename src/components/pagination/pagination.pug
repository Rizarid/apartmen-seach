mixin pagination(maxValue, paginationText)
  .pagination
    .pagination__pages
      .pagination__prevButton
        span
      .pagination__page.active 1
      .pagination__page 2
      .pagination__page 3
      .pagination__page ...
      .pagination__page #{maxValue} 
      .pagination__nextButton 
        span

    
    .pagination__textConteiner 
      p.pagination__text #{paginationText}