mixin head(options = {})
  -
    const { 
     description = 'Лучшие номера для вашей работы, отдыха и просто вдохновения', 
      keywords = 'номер, отель, номер в отеле, поис номеров, гостиница', 
      title = 'Найдём номера под ваши пожелания'  
    } = options
  head
    meta(charset = "UTF-8")
    meta(name = "viewport", content = "width = device-width, initial-scale = 1.0")
    meta(name = "description", content = description)
    meta(name = "keywords", content = keywords)

    title #{title} 

  body